# Telegram Configuration (Required)
WNB_TELEGRAM_BOT_TOKEN=your_telegram_bot_token
WNB_TELEGRAM_PASSWORD=your_secure_password
WNB_TELEGRAM_ADMIN_USER_ID=123456789

# Caiyun Weather API Configuration (Required)
WNB_CAIYUN_API_TOKEN=your_caiyun_api_token
WNB_CAIYUN_LATITUDE=39.9042
WNB_CAIYUN_LONGITUDE=116.4074
WNB_CAIYUN_TIMEOUT=30

# LLM Configuration (Required)
WNB_LLM_PROVIDER=openai
WNB_LLM_API_KEY=your_openai_api_key
WNB_LLM_MODEL=gpt-4
WNB_LLM_BASE_URL=
WNB_LLM_TIMEOUT=60

# Schedule Configuration (Optional - defaults provided)
WNB_SCHEDULE_CHECK_WEATHER_CRON=0 7,12,18 * * *
WNB_SCHEDULE_TIMEZONE=Asia/Shanghai

# Detection Thresholds (Optional - defaults provided)
WNB_DETECTION_RAIN_PROBABILITY_THRESHOLD=0.3
WNB_DETECTION_TEMPERATURE_HIGH_THRESHOLD=35.0
WNB_DETECTION_TEMPERATURE_LOW_THRESHOLD=0.0
WNB_DETECTION_AQI_THRESHOLD=150

# Logging Configuration (Optional - defaults provided)
WNB_LOGGING_LEVEL=info
WNB_LOGGING_PRETTY_PRINT=false
